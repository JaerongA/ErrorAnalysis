    <!-- http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Guideline_Error -->

    <owl:Class rdf:about="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Guideline_Error">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Annotation_Error"/>
        <CCEE_Taxonomy:definition>errors occur due to conflicts between NLP definition and annotation guideline definition. E.g., the version of guideline does not align with NLP version.</CCEE_Taxonomy:definition>
        <CCEE_Taxonomy:evaluation_level>mention, sentence</CCEE_Taxonomy:evaluation_level>
        <CCEE_Taxonomy:evaluation_subject>human</CCEE_Taxonomy:evaluation_subject>
        <CCEE_Taxonomy:example>Guideline definition: encephalopathy = delirium positive
NLP definition: encephalopathy = CAM_D</CCEE_Taxonomy:example>
        <rdfs:label>Guideline_Error</rdfs:label>
    </owl:Class>
    


