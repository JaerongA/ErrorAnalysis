    <!-- http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Logic_Pattern_and_Rule -->

    <owl:Class rdf:about="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Logic_Pattern_and_Rule">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Logic_Error"/>
        <CCEE_Taxonomy:definition>logic errors occur when NLP model falsely make classification or summarization due to logic or rule patterns.</CCEE_Taxonomy:definition>
        <CCEE_Taxonomy:evaluation_level>mention, sentence, document, patient</CCEE_Taxonomy:evaluation_level>
        <CCEE_Taxonomy:evaluation_subject>model, human</CCEE_Taxonomy:evaluation_subject>
        <CCEE_Taxonomy:example>Silent cerebrovascular disease = positive mention of brain infarction 
Correction: positive mention of brain infarction without prior history of stroke

CAM Delirium positive = CAM_BCD
Correction: CAM Delirium positive = CAM_ABCD or CAM_ABC or CAM_ABD</CCEE_Taxonomy:example>
        <rdfs:label>Logic_Pattern_Rule</rdfs:label>
    </owl:Class>
    


