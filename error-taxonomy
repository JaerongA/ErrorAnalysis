    <!-- http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Absence_of_Context -->

    <owl:Class rdf:about="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Absence_of_Context">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Contextual_Error"/>
        <CCEE_Taxonomy:definition>NLP model falsely or partially captures concepts due to lack of enough context to determine the certainty, status and subject of the expression.</CCEE_Taxonomy:definition>
        <CCEE_Taxonomy:evaluation_level>mention, sentence</CCEE_Taxonomy:evaluation_level>
        <CCEE_Taxonomy:evaluation_subject>model</CCEE_Taxonomy:evaluation_subject>
        <CCEE_Taxonomy:example>‘Confusion’ (occurs as a single concept without context) 
Correction: patient has a constant confused look displayed on his/her face.</CCEE_Taxonomy:example>
        <rdfs:label>Absence_of_Context</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Annotation_Error -->

    <owl:Class rdf:about="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Annotation_Error">
        <CCEE_Taxonomy:definition>Human error occurred during corpus annotation, which is a process involving the manual review, judgment, marking, and labeling of linguistic and clinical concepts from unstructured text.</CCEE_Taxonomy:definition>
        <CCEE_Taxonomy:evaluation_subject>human</CCEE_Taxonomy:evaluation_subject>
