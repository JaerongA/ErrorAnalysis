    </owl:Class>
    


    <!-- http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Nomonyms -->

    <owl:Class rdf:about="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Nomonyms">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Semantic_Error"/>
        <CCEE_Taxonomy:definition>NLP model fails to capture concepts due to words that are spelled and pronounced the same but have different meanings.</CCEE_Taxonomy:definition>
        <CCEE_Taxonomy:evaluation_subject>model, human</CCEE_Taxonomy:evaluation_subject>
        <CCEE_Taxonomy:example>Fall: 
Fall asleep
Redwood falls
Water falls</CCEE_Taxonomy:example>
        <rdfs:label>Homonyms</rdfs:label>
    </owl:Class>
    


