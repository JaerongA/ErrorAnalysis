    <!-- http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Differential_Diagnosis -->

    <owl:Class rdf:about="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Differential_Diagnosis">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Certainty"/>
        <CCEE_Taxonomy:definition>A model&apos;s false classification of concepts is contingent on differential diagnosis, a process where clinicians consider various possible conditions or diseases that could explain a patient&apos;s symptoms before making a final diagnosis.</CCEE_Taxonomy:definition>
        <CCEE_Taxonomy:evaluation_level>sentence, section</CCEE_Taxonomy:evaluation_level>
        <CCEE_Taxonomy:evaluation_subject>model, human</CCEE_Taxonomy:evaluation_subject>
        <rdfs:label>Differential_Diagnosis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Exclusion -->

    <owl:Class rdf:about="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Exclusion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sfu3/ontologies/2023/9/CCEE_Taxonomy#Contextual_Error"/>
        <CCEE_Taxonomy:definition>NLP model falsely captures concepts due to the concepts occur in a wrong context.</CCEE_Taxonomy:definition>
        <CCEE_Taxonomy:evaluation_level>mention, sentence; document</CCEE_Taxonomy:evaluation_level>
        <CCEE_Taxonomy:evaluation_subject>model</CCEE_Taxonomy:evaluation_subject>
        <CCEE_Taxonomy:example>I was called to evaluate the patient for an episode of confusion
Patient was screened for confusion
Fall prevention education
The nurse disconnected the call
*The aforementioned concepts need to be excluded because they do not reflect patient’s disease status</CCEE_Taxonomy:example>
        <rdfs:label>Exclusion</rdfs:label>
    </owl:Class>
    


